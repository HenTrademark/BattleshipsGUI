                    <ComboBox Grid.Row="1" Grid.Column="0" PlaceholderText="Choose for me" HorizontalAlignment="Center"
                              SelectedIndex="{Binding Carriers, Mode=OneWayToSource}">
                        <ComboBoxItem Content="1" /> <ComboBoxItem Content="2" /> <ComboBoxItem Content="3" />
                    </ComboBox>
                    <TextBlock Grid.Row="0" Grid.Column="1" Text="Destroyers (4 squares):" HorizontalAlignment="Center" />
                    <ComboBox Grid.Row="1" Grid.Column="1" PlaceholderText="Choose for me" HorizontalAlignment="Center"
                              SelectedIndex="{Binding Destroyers, Mode=OneWayToSource}">
                        <ComboBoxItem Content="1" /> <ComboBoxItem Content="2" /> <ComboBoxItem Content="3" />
                        <ComboBoxItem Content="4" />
                    </ComboBox>
                    <TextBlock Grid.Row="0" Grid.Column="2" Text="Ships (3 squares):" HorizontalAlignment="Center" />
                    <ComboBox Grid.Row="1" Grid.Column="2" PlaceholderText="Choose for me" HorizontalAlignment="Center"
                              SelectedIndex="{Binding Ships, Mode=OneWayToSource}">
                        <ComboBoxItem Content="1" /> <ComboBoxItem Content="2" /> <ComboBoxItem Content="3" />
                        <ComboBoxItem Content="4" /> <ComboBoxItem Content="5" /> <ComboBoxItem Content="6" />
                    </ComboBox>
                    <TextBlock Grid.Row="0" Grid.Column="3" Text="Patrols (2 squares):" HorizontalAlignment="Center" />
                    <ComboBox Grid.Row="1" Grid.Column="3" PlaceholderText="Choose for me" HorizontalAlignment="Center"
                              SelectedIndex="{Binding Patrols, Mode=OneWayToSource}">
                        <ComboBoxItem Content="1" /> <ComboBoxItem Content="2" /> <ComboBoxItem Content="3" />
                        <ComboBoxItem Content="4" /> <ComboBoxItem Content="5" /> <ComboBoxItem Content="6" />
                        <ComboBoxItem Content="7" /> <ComboBoxItem Content="8" /> <ComboBoxItem Content="9" />
                        <ComboBoxItem Content="0" />
                    </ComboBox>
